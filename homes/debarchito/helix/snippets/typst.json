  "figure": {
    "prefix": "FIG",
    "body": [
      "#figure(",
      " ${1:content} ,",
      "caption : [${2:caption}]",
      ")<${3:reference}>"
    ],
    "description": "Figure"
  },
  "table": {
    "prefix": "TABLE",
    "body": [
      "#table(",
      "  columns: ${1:columns},",
      "  //// align: ,",
      "  table.header[${2:header}],",
      "  [${3:data}]",
      ")"
    ],
